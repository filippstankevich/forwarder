@{
    ViewBag.Title = "Shipping";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model Forwarder.Models.ShipmentModel
<div class="row">
    @Html.ActionLink("Назад", "TransportationEdit", new { id = Model.Id })
    @using (Html.BeginForm("ExportData", "Transportation", FormMethod.Post,
                           new { id = "upload_form", enctype = "multipart/form-data" }))
    {
        <label>Импорт из Excel</label>                    
        <input type="file" name='file' id="file"  onchange="submit"/><br /> 
        @Html.HiddenFor(m => m.Id)                 
    }
</div>
<table id = "shipping"></table>
